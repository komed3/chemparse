  },
  "scripts": {
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:mjs": "tsc --project tsconfig.mjs.json",
    "build:types": "tsc --project tsconfig.d.json",
    "build": "npm run build:cjs && npm run build:mjs && npm run build:types"
